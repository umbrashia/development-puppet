{
  "name": "Puppet Login",
  "version": "1.0.4",
  "description": "This extention for development login check with using email id and those email id saved automaticaly in this plugin",
  "manifest_version": 2,
  "author":"Umbrashia Corporation (shantanu sharma)",
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/background-js/style-changer.js"
    ],
    "persistent": false
  },

  "options_ui": {
    "page": "template/export.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "default_popup": "template/home-popup.html"
  }
}