{
  "name": "Getting Started ExampleMM",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "permissions": [
   "declarativeContent", "storage","activeTab"
  ],
  "background": {
    "scripts": [
      "js/background-js/style-changer.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "plugins/js/jquery-3.4.1.min.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "images/get_started48.png",
    "default_popup": "template/home-popup.html"
  }
}